{
  "private": true,
  "type": "module",
  "name": "empty-template-project",
  "version": "1.1.1",
  "description": "Repo Template: parcel, mocha-chai, eslint.",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "browserslist": [
    "baseline 2025"
  ],
  "prettier": "@epreston/prettier-config",
  "packageManager": "npm@11.7.0",
  "engines": {
    "node": "^20.19.0 || ^22.21.0 || >=24.12.0"
  },
  "scripts": {
    "clean": "rimraf -g -v dist/*",
    "start": "parcel serve ./src/index.html",
    "build": "rimraf -g dist/* && parcel build ./src/index.html",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.?(c|m)[tj]s\"",
    "test": "mocha"
  },
  "devDependencies": {
    "@epreston/eslint-config": "^2.1.1",
    "@epreston/prettier-config": "^2.1.0",
    "@epreston/tsconfig": "^1.2.1",
    "chai": "^6.2.2",
    "eslint": "^9.39.2",
    "mocha": "^11.7.5",
    "parcel": "^2.16.3",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2"
  }
}
