{
  "private": true,
  "type": "module",
  "name": "empty-template-project",
  "version": "1.1.0",
  "description": "Repo Template: parcel, mocha-chai, eslint.",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "browserslist": [
    "Chrome >= 120",
    "ChromeAndroid >= 120",
    "Edge >= 120",
    "Firefox >= 121",
    "FirefoxAndroid >= 122",
    "Safari >= 17.1",
    "iOS >= 17.1",
    "not dead"
  ],
  "packageManager": "npm@10.7.0",
  "prettier": "@epreston/prettier-config",
  "scripts": {
    "clean": "rimraf -g dist/*",
    "start": "parcel serve ./src/index.html",
    "build": "rimraf -g dist/* && parcel build ./src/index.html",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.?(c|m)[tj]s\"",
    "test": "mocha"
  },
  "devDependencies": {
    "@epreston/eslint-config": "^2.1.1",
    "@epreston/prettier-config": "^2.1.0",
    "@epreston/tsconfig": "^1.2.1",
    "chai": "^5.1.1",
    "eslint": "^9.7.0",
    "mocha": "^10.7.0",
    "parcel": "^2.12.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1"
  },
  "engines": {
    "node": "^18.20.4 || ^20.15.1 || >=22.5.0"
  }
}
