{
  "private": true,
  "type": "module",
  "name": "empty-template-project",
  "version": "1.1.1",
  "description": "Repo Template: parcel, mocha-chai, eslint.",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "browserslist": [
    "Chrome >= 124",
    "ChromeAndroid >= 124",
    "Edge >= 124",
    "Firefox >= 124",
    "FirefoxAndroid >= 124",
    "Safari >= 17.4",
    "iOS >= 17.4",
    "not dead"
  ],
  "prettier": "@epreston/prettier-config",
  "packageManager": "npm@10.8.2",
  "engines": {
    "node": "^18.20.4 || ^20.17.0 || >=22.7.0"
  },
  "scripts": {
    "clean": "rimraf -g dist/*",
    "start": "parcel serve ./src/index.html",
    "build": "rimraf -g dist/* && parcel build ./src/index.html",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.?(c|m)[tj]s\"",
    "test": "mocha"
  },
  "devDependencies": {
    "@epreston/eslint-config": "^2.1.1",
    "@epreston/prettier-config": "^2.1.0",
    "@epreston/tsconfig": "^1.2.1",
    "chai": "^5.2.1",
    "eslint": "^9.32.0",
    "mocha": "^11.7.1",
    "parcel": "^2.15.4",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1"
  }
}
