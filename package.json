{
  "private": true,
  "name": "empty-template-project",
  "version": "1.0.0",
  "description": "Repo Template: parcel, mocha-chai, eslint.",
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "browserslist": "Chrome >= 112, ChromeAndroid >= 112, Edge >= 112, Firefox >= 112, FirefoxAndroid  >= 112,  Safari >= 16.4, iOS >= 16.4, not dead",
  "eslintConfig": {
    "root": true,
    "extends": "@epreston/eslint-config"
  },
  "prettier": "@epreston/prettier-config",
  "scripts": {
    "clean": "rimraf dist/*",
    "start": "parcel serve ./src/index.html",
    "build": "rimraf dist/* && parcel build ./src/index.html",
    "lint": "eslint --ext .js --report-unused-disable-directives .",
    "test": "mocha"
  },
  "devDependencies": {
    "@epreston/eslint-config": "^1.1.0",
    "@epreston/prettier-config": "^1.0.0",
    "@epreston/tsconfig": "^1.0.1",
    "chai": "^4.3.10",
    "eslint": "^8.53.0",
    "mocha": "^10.2.0",
    "parcel": "^2.10.2"
  },
  "engines": {
    "node": "^18.18.0 || >=20.9.0"
  }
}
