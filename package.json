{
  "private": true,
  "name": "empty-template-project",
  "version": "1.0.0",
  "description": "Repo Template: parcel, mocha-chai, eslint.",
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "browserslist": [
    "Chrome >= 112",
    "ChromeAndroid >= 112",
    "Edge >= 112",
    "Firefox >= 112",
    "FirefoxAndroid >= 112",
    "Safari >= 16.4",
    "iOS >= 16.4",
    "not dead"
  ],
  "eslintConfig": {
    "root": true,
    "extends": "@epreston/eslint-config"
  },
  "prettier": "@epreston/prettier-config",
  "scripts": {
    "clean": "rimraf -g dist/*",
    "start": "parcel serve ./src/index.html",
    "build": "rimraf -g dist/* && parcel build ./src/index.html",
    "lint": "eslint --ext .js --report-unused-disable-directives .",
    "test": "mocha"
  },
  "devDependencies": {
    "@epreston/eslint-config": "^1.1.1",
    "@epreston/prettier-config": "^2.0.0",
    "@epreston/tsconfig": "^1.1.1",
    "chai": "^5.0.0",
    "eslint": "^8.56.0",
    "mocha": "^10.2.0",
    "parcel": "^2.11.0",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": "^18.18.0 || >=20.9.0"
  }
}
